<Application x:Class="Telegram.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Telegram"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="HamburgerMenu" UriSource="icons\ham.png"></BitmapImage>
            <BitmapImage x:Key="RedCircle" UriSource="icons\red circle.png"></BitmapImage>
            <BitmapImage x:Key="PushPin" UriSource="icons\push pin.png"></BitmapImage>
        </ResourceDictionary>
    </Application.Resources>
</Application>
